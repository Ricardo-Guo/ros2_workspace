    <origin xyz="0 0 0.20" rpy="-1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.9" upper="2.9" effort="30" velocity="2"/>
  </joint>

  <!-- ================= LINK 6 ================= -->
  <link name="panda_link6">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <origin xyz="0 0 0.03"/>
      <material name="dark_grey"/>
    </visual>
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.12"/>
      </geometry>
      <origin xyz="0 0 0.09"/>
      <material name="light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.12"/>
      </geometry>
      <origin xyz="0 0 0.09"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0.08"/>
      <inertia ixx="0.03" iyy="0.03" izz="0.008"
               ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="panda_joint6" type="revolute">
    <parent link="panda_link5"/>
    <child link="panda_link6"/>
    <origin xyz="0 0 0.18" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="3.75" effort="30" velocity="2"/>
  </joint>

  <!-- ================= LINK 7 ================= -->
  <link name="panda_link7">
    <visual>
      <geometry>
        <cylinder radius="0.045" length="0.06"/>
      </geometry>
      <origin xyz="0 0 0.03"/>
      <material name="dark_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.06"/>
      </geometry>
      <origin xyz="0 0 0.03"/>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <origin xyz="0 0 0.03"/>
      <inertia ixx="0.02" iyy="0.02" izz="0.005"
               ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="panda_joint7" type="revolute">
    <parent link="panda_link6"/>
    <child link="panda_link7"/>
    <origin xyz="0 0 0.12" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.9" upper="2.9" effort="30" velocity="2"/>
  </joint>

  <!-- ================= END EFFECTOR FINAL STABLE (GAZEBO SAFE) ================= -->


  <!-- ================= flange ================= -->
  <link name="ee_flange">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.02"/>
      </geometry>
      <origin xyz="0 0 0.01"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.02"/>
      </geometry>
      <origin xyz="0 0 0.01"/>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <origin xyz="0 0 0.01"/>
      <inertia ixx="0.0002" iyy="0.0002" izz="0.0002"
              ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="ee_fixed" type="fixed">
    <parent link="panda_link7"/>
    <child link="ee_flange"/>
    <origin xyz="0 0 0.06"/>
  </joint>



  <!-- ================= base ================= -->
  <link name="gripper_base">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.04"/>
      </geometry>
      <origin xyz="0 0 0.02"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.04"/>
      </geometry>
      <origin xyz="0 0 0.02"/>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <origin xyz="0 0 0.02"/>
      <inertia ixx="0.0005" iyy="0.0005" izz="0.0005"
              ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="gripper_base_fixed" type="fixed">
    <parent link="ee_flange"/>
    <child link="gripper_base"/>
    <origin xyz="0 0 0.02"/>
  </joint>



  <!-- ================= LEFT FINGER (CONNECTED + LONG) ================= -->
<link name="finger_left">
  <visual>
    <geometry>
      <box size="0.01 0.01 0.12"/>
    </geometry>

    <!-- ⭐⭐⭐ 关键：半长 0.06（不是 0.12！） -->
    <origin xyz="0 0 0.06"/>
  </visual>

  <collision>
    <geometry>
      <box size="0.01 0.01 0.12"/>
    </geometry>
    <origin xyz="0 0 0.06"/>
  </collision>

  <inertial>
    <mass value="0.08"/>
    <origin xyz="0 0 0.06"/>
    <inertia ixx="0.00005" iyy="0.00005" izz="0.00005"
             ixy="0" ixz="0" iyz="0"/>
  </inertial>
</link>

<joint name="finger_left_joint" type="revolute">
  <parent link="gripper_base"/>
  <child link="finger_left"/>
  <origin xyz="0 0.03 0"/>
  <axis xyz="1 0 0"/>
  <limit lower="0" upper="0.7" effort="10" velocity="2.0"/>
</joint>



<!-- ================= RIGHT FINGER ================= -->
<link name="finger_right">
  <visual>
    <geometry>
      <box size="0.01 0.01 0.12"/>
    </geometry>
    <origin xyz="0 0 0.06"/>
  </visual>

  <collision>
    <geometry>
      <box size="0.01 0.01 0.12"/>
    </geometry>
    <origin xyz="0 0 0.06"/>
  </collision>

  <inertial>
    <mass value="0.08"/>
    <origin xyz="0 0 0.06"/>
    <inertia ixx="0.00005" iyy="0.00005" izz="0.00005"
             ixy="0" ixz="0" iyz="0"/>
  </inertial>
</link>

<joint name="finger_right_joint" type="revolute">
  <parent link="gripper_base"/>
  <child link="finger_right"/>
  <origin xyz="0 -0.03 0"/>
  <axis xyz="-1 0 0"/>
  <limit lower="0" upper="0.7" effort="10" velocity="2.0"/>
</joint>



<!-- ================= TCP（重新精确计算）================= -->
<link name="tool0"/>

<joint name="tool0_fixed" type="fixed">
  <parent link="gripper_base"/>
  <child link="tool0"/>

  <!-- ⭐ tip = 0.12 -->
  <origin xyz="0 0 0.12"/>
</joint>



  
  <!-- ================= TRANSMISSIONS ================= -->

<transmission name="panda_joint1_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="panda_joint1">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
  </joint>
  <actuator name="panda_joint1_motor">
    <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>

<transmission name="panda_joint2_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="panda_joint2">
