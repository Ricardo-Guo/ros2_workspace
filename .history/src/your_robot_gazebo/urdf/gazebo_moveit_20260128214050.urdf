    </joint>
  </ros2_control>

  <!-- ================= GAZEBO ROS2 CONTROL PLUGIN ================= -->
  <gazebo>
  <plugin name="gazebo_ros2_control"
          filename="libgazebo_ros2_control.so">
    <parameters>
      $(find your_robot_gazebo)/config/moveit_controllers.yaml
    </parameters>
  </plugin>
</gazebo>


</robot>