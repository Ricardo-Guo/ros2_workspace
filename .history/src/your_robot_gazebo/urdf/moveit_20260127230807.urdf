  <joint name="panda_joint5" type="revolute">
    <parent link="panda_link4"/>
    <child link="panda_link5"/>
    <origin xyz="0 0 0.20" rpy="-1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.9" upper="2.9" effort="30" velocity="2"/>
  </joint>

  <!-- ================= LINK 6 ================= -->
  <link name="panda_link6">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <origin xyz="0 0 0.03"/>
      <material name="dark_grey"/>
    </visual>
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.12"/>
      </geometry>
      <origin xyz="0 0 0.09"/>
      <material name="light_grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.12"/>
      </geometry>
      <origin xyz="0 0 0.09"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0.08"/>
      <inertia ixx="0.03" iyy="0.03" izz="0.008"
